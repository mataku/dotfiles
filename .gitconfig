[user]
	name = Homma Takuma
	email = nagomimatcha@gmail.com
[color]
  ui = true
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  st = status
  co = checkout
  unstage = reset HEAD
  b = branch
  al = config --get-regexp alias.*
  cm = commit
  wip = commit --allow-empty -m 'WIP'
[merge "mergepbx"]
	name = Xcode project files merger
	driver = mergepbx %O %A %B
